--ESP MOD
local HomeFrame = sections["Visual"]
local player = game.Players.LocalPlayer
local userInputService = game:GetService("UserInputService")

local espEnabled = false
local menuVisible = false
local espObjects = {}

-- N√∫t m·ªü ESP Menu
local espButton = Instance.new("TextButton", HomeFrame)
espButton.Size = UDim2.new(0, 300, 0, 40)
espButton.Position = UDim2.new(0, 10, 0, 10)
espButton.Text = "üëøMENU ESP MOD"
espButton.BackgroundColor3 = Color3.new(0.35, 0, 0)
espButton.TextColor3 = Color3.new(139, 0, 0)

-- Menu ESP
local screenGui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
local espMenu = Instance.new("Frame", screenGui)
espMenu.Size = UDim2.new(0, 150, 0, 100)
espMenu.Position = UDim2.new(0.5, -75, 0.2, 0)
espMenu.BackgroundColor3 = Color3.new(139, 0, 0)
espMenu.BorderSizePixel = 2
espMenu.Active = true
espMenu.Visible = false

-- N√∫t b·∫≠t/t·∫Øt ESP
local toggleButton = Instance.new("TextButton", espMenu)
toggleButton.Size = UDim2.new(1, 0, 0.6, 0)
toggleButton.Position = UDim2.new(0, 0, 0.2, 0)
toggleButton.Text = "B·∫≠t ESP"
toggleButton.BackgroundColor3 = Color3.new(0.5, 0.5, 0.5)
toggleButton.TextColor3 = Color3.new(1, 1, 1)

-- Fix l·ªói PE: ƒê·∫£m b·∫£o menu c√≥ th·ªÉ hi·ªán/t·∫Øt ƒë√∫ng c√°ch
local function updateMenuVisibility()
    espMenu.Visible = menuVisible
end

espButton.MouseButton1Click:Connect(function()
    menuVisible = not menuVisible
    updateMenuVisibility()
end)

-- C∆° ch·∫ø k√©o th·∫£ (ho·∫°t ƒë·ªông tr√™n c·∫£ PC & PE)
local dragging, dragStart, startPos

espMenu.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = espMenu.Position
    end
end)

espMenu.InputChanged:Connect(function(input)
    if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
        local delta = input.Position - dragStart
        espMenu.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

userInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = false
    end
end)

-- Ch·ª©c nƒÉng ESP
local function createESP(obj)
    if obj:IsA("Model") then
        local highlight = Instance.new("Highlight")
        highlight.Adornee = obj
        highlight.FillColor = Color3.new(1, 0, 0)
        highlight.OutlineColor = Color3.new(1, 0, 0)
        highlight.FillTransparency = 1
        highlight.OutlineTransparency = 0
        highlight.Parent = obj

        local billboardGui = Instance.new("BillboardGui")
        billboardGui.Adornee = obj
        billboardGui.Size = UDim2.new(0, 100, 0, 50)
        billboardGui.StudsOffset = Vector3.new(0, 3, 0)
        billboardGui.AlwaysOnTop = true
        billboardGui.Parent = obj

        local textLabel = Instance.new("TextLabel")
        textLabel.Size = UDim2.new(1, 0, 1, 0)
        textLabel.BackgroundTransparency = 1
        textLabel.TextColor3 = Color3.new(1, 0, 0)
        textLabel.TextSize = 16
        textLabel.Font = Enum.Font.SourceSansBold
        textLabel.Text = obj.Name
        textLabel.Parent = billboardGui

        table.insert(espObjects, highlight)
        table.insert(espObjects, billboardGui)
    end
end

-- T√¨m v√† t·∫°o ESP
local function findAndESP()
    local folders = {
        game.Workspace:FindFirstChild("RuntimeEntities"),
        game.Workspace:FindFirstChild("RuntimeEnemies"),
        game.Workspace:FindFirstChild("NightEnemies"),
        game.Workspace:FindFirstChild("Animals")
    }

    for _, folder in pairs(folders) do
        if folder then
            for _, obj in pairs(folder:GetChildren()) do
                createESP(obj)
            end
            folder.ChildAdded:Connect(createESP)
        end
    end
end

local function toggleESP(state)
    if state then
        findAndESP()
    else
        for _, esp in pairs(espObjects) do
            esp:Destroy()
        end
        espObjects = {}
    end
end

toggleButton.MouseButton1Click:Connect(function()
    espEnabled = not espEnabled
    toggleButton.Text = espEnabled and "T·∫Øt ESP" or "B·∫≠t ESP"
    toggleButton.BackgroundColor3 = espEnabled and Color3.new(0, 1, 0) or Color3.new(0.5, 0.5, 0.5)
    toggleESP(espEnabled)
end)

