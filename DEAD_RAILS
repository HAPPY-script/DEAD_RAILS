toggleItemButton.TextColor3 = Color3.new(1, 1, 1)

-- Ch·ª©c nƒÉng k√©o th·∫£ menu ESP Item
local dragging, dragInput, dragStart, startPos

local function updateInput(input)
    local delta = input.Position - dragStart
    espItemMenu.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

espItemMenu.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = espItemMenu.Position

        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

espItemMenu.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        dragInput = input
    end
end)

userInputService.InputChanged:Connect(function(input)
    if dragging and input == dragInput then
        updateInput(input)
    end
end)

-- H√†m t·∫°o ESP
local function createESPItem(obj)
    local attachPart = obj:FindFirstChildWhichIsA("BasePart")
    if attachPart then
        -- T·∫°o highlight
        local highlight = Instance.new("Highlight")
        highlight.Adornee = obj
        highlight.FillColor = Color3.new(0, 1, 0) -- Xanh l√° c√¢y
        highlight.OutlineColor = Color3.new(0, 1, 0)
        highlight.FillTransparency = 1
        highlight.OutlineTransparency = 0
        highlight.Parent = obj

        -- T·∫°o t√™n Item
        local billboard = Instance.new("BillboardGui")
        billboard.Adornee = attachPart
        billboard.Size = UDim2.new(0, 200, 0, 50)
        billboard.StudsOffset = Vector3.new(0, 2, 0)
        billboard.AlwaysOnTop = true
        billboard.Parent = obj

        local textLabel = Instance.new("TextLabel", billboard)
        textLabel.Size = UDim2.new(0.3, 0, 0.3, 0)
        textLabel.BackgroundTransparency = 1
        textLabel.TextColor3 = Color3.new(0, 1, 0) -- M√†u xanh l√°
        textLabel.TextStrokeTransparency = 0
        textLabel.TextStrokeColor3 = Color3.new(0, 0, 0)
        textLabel.TextScaled = true
        textLabel.Font = Enum.Font.SourceSansBold
        textLabel.Text = obj.Name

        -- L∆∞u l·∫°i highlight v√† t√™n ƒë·ªÉ xo√° khi c·∫ßn
        table.insert(espItemObjects, {highlight, billboard})
    end
end

-- H√†m b·∫≠t/t·∫Øt ESP
local function toggleESPItem(state)
    if state then
        local itemsFolder = game.Workspace:FindFirstChild("RuntimeItems")
        if itemsFolder then
            for _, obj in pairs(itemsFolder:GetDescendants()) do
                if obj:IsA("Model") then
                    createESPItem(obj)
                end
            end
            -- Theo d√µi item m·ªõi xu·∫•t hi·ªán
            childAddedConnection = itemsFolder.ChildAdded:Connect(function(obj)
                if obj:IsA("Model") then
                    createESPItem(obj)
                end
            end)
        else
            warn("Kh√¥ng t√¨m th·∫•y th∆∞ m·ª•c RuntimeItems!")
        end
    else
        -- Xo√° to√†n b·ªô ESP
        for _, espData in pairs(espItemObjects) do
            if espData[1] then espData[1]:Destroy() end -- X√≥a highlight
            if espData[2] then espData[2]:Destroy() end -- X√≥a t√™n
        end
        espItemObjects = {}

        -- Ng·∫Øt k·∫øt n·ªëi s·ª± ki·ªán th√™m item
        if childAddedConnection then
            childAddedConnection:Disconnect()
            childAddedConnection = nil
        end
    end
end

-- N√∫t b·∫≠t/t·∫Øt ESP Item
toggleItemButton.MouseButton1Click:Connect(function()
    espItemEnabled = not espItemEnabled
    toggleItemButton.Text = espItemEnabled and "T·∫Øt ESP Item" or "B·∫≠t ESP Item"
    toggleItemButton.BackgroundColor3 = espItemEnabled and Color3.new(0, 1, 0) or Color3.new(0.5, 0.5, 0.5)
    toggleESPItem(espItemEnabled)
end)

-- Khi b·∫•m v√†o n√∫t tr√™n tab Home, hi·ªán/·∫©n menu ESP ITEM
espItemButton.MouseButton1Click:Connect(function()
    itemMenuVisible = not itemMenuVisible
    screenGui.Enabled = itemMenuVisible  -- S·ª≠ d·ª•ng Enabled thay v√¨ Visible
end)

