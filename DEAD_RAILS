--TP KEY PC
local HomeFrame = sections["Home"] -- L·∫•y ph·∫ßn ch·ª©a n·ªôi dung c·ªßa tab "Home"
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local userInputService = game:GetService("UserInputService")
local runService = game:GetService("RunService")

-- Bi·∫øn tr·∫°ng th√°i
local teleportEnabled = false
local selectedKey = nil

-- T·∫°o n√∫t b·∫≠t/t·∫Øt ch·ª©c nƒÉng
local TeleportButton = Instance.new("TextButton", HomeFrame)
TeleportButton.Size = UDim2.new(0, 190, 0, 40)
TeleportButton.Position = UDim2.new(0, 10, 0, 60)
TeleportButton.Text = "[üñ•Ô∏è]ON Teleport‚ö°"
TeleportButton.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
TeleportButton.TextColor3 = Color3.new(1, 1, 1)

-- T·∫°o √¥ ch·ªçn ph√≠m
local KeySelectBox = Instance.new("TextButton", HomeFrame)
KeySelectBox.Size = UDim2.new(0, 100, 0, 40)
KeySelectBox.Position = UDim2.new(0, 210, 0, 60)
KeySelectBox.Text = "SELECT KEY"
KeySelectBox.BackgroundColor3 = Color3.new(0.4, 0.4, 0.4)
KeySelectBox.TextColor3 = Color3.new(1, 1, 1)

-- Ch·ªçn ph√≠m
KeySelectBox.MouseButton1Click:Connect(function()
    KeySelectBox.Text = "Select key..."
    local keyConnection
    keyConnection = userInputService.InputBegan:Connect(function(input)
        selectedKey = input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode.Name or input.UserInputType.Name
        KeySelectBox.Text = selectedKey
        keyConnection:Disconnect()
    end)
end)

-- Ch·ª©c nƒÉng teleport
local function teleportToMouse()
    if teleportEnabled and selectedKey then
        local mouse = player:GetMouse()
        local targetPosition = Vector3.new(mouse.Hit.Position.X, mouse.Hit.Position.Y, mouse.Hit.Position.Z)
        local character = player.Character
        if character and character:FindFirstChild("HumanoidRootPart") then
            local rootPart = character.HumanoidRootPart
            local distance = (rootPart.Position - targetPosition).Magnitude
            if distance <= 20 then
                rootPart.CFrame = CFrame.new(targetPosition)
            end
        end
    end
end

-- B·∫≠t/t·∫Øt ch·ª©c nƒÉng
TeleportButton.MouseButton1Click:Connect(function()
    teleportEnabled = not teleportEnabled
    TeleportButton.Text = teleportEnabled and "[üñ•Ô∏è]OFF Teleport‚ö°" or "[üñ•Ô∏è]ON Teleport‚ö°"
    TeleportButton.BackgroundColor3 = teleportEnabled and Color3.new(0, 1, 0) or Color3.new(0.2, 0.2, 0.2)
end)

-- Ki·ªÉm tra khi b·∫•m ph√≠m ƒë·ªÉ teleport
userInputService.InputBegan:Connect(function(input, gameProcessed)
    if not gameProcessed and teleportEnabled and selectedKey then
        local inputName = input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode.Name or input.UserInputType.Name
        if inputName == selectedKey then
            teleportToMouse()
        end
    end
end)

-- Reset khi ch·∫øt
player.CharacterAdded:Connect(function(newCharacter)
    teleportEnabled = false
    selectedKey = nil
    TeleportButton.Text = "[üñ•Ô∏è]ON Teleport‚ö°"
    TeleportButton.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
    KeySelectBox.Text = "SELECT KEY"
end)

