--Light
local VisualFrame = sections["Visual"] -- L·∫•y ph·∫ßn ch·ª©a n·ªôi dung c·ªßa tab "Visual"
local lighting = game:GetService("Lighting")

local lightEnabled = false -- Tr·∫°ng th√°i b·∫≠t/t·∫Øt √°nh s√°ng

-- T·∫°o n√∫t Light
local lightButton = Instance.new("TextButton", VisualFrame)
lightButton.Size = UDim2.new(0, 300, 0, 40)
lightButton.Position = UDim2.new(0, 10, 0, 110)
lightButton.Text = "‚òÄLight OFF"
lightButton.BackgroundColor3 = Color3.new(0.3, 0.3, 0.3)
lightButton.TextColor3 = Color3.new(1, 1, 1)

-- Ch·ª©c nƒÉng b·∫≠t/t·∫Øt √°nh s√°ng
local function toggleLight(state)
    if state then
        lighting.Brightness = 3 -- TƒÉng ƒë·ªô s√°ng
        lighting.Ambient = Color3.new(1, 1, 1) -- L√†m s√°ng m√¥i tr∆∞·ªùng
        lighting.OutdoorAmbient = Color3.new(1, 1, 1) -- Gi·∫£m ƒë·ªô t·ªëi b√™n ngo√†i
        lighting.GlobalShadows = false -- T·∫Øt b√≥ng t·ªëi
    else
        lighting.Brightness = 1 -- Tr·∫£ v·ªÅ ƒë·ªô s√°ng ban ƒë·∫ßu
        lighting.Ambient = Color3.new(0.5, 0.5, 0.5)
        lighting.OutdoorAmbient = Color3.new(0.5, 0.5, 0.5)
        lighting.GlobalShadows = true -- B·∫≠t b√≥ng t·ªëi l·∫°i
    end
end

-- N√∫t b·∫≠t/t·∫Øt √°nh s√°ng
lightButton.MouseButton1Click:Connect(function()
    lightEnabled = not lightEnabled
    lightButton.Text = lightEnabled and "‚òÄLight ON" or "‚òÄLight OFF"
    lightButton.BackgroundColor3 = lightEnabled and Color3.new(1, 1, 0.39) or Color3.new(0.3, 0.3, 0.3)
    toggleLight(lightEnabled)
end)

