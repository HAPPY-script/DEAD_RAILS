-- üü¢ B·∫¨T/T·∫ÆT CH·ª®C NƒÇNG AIM
local function ToggleAim()
    silentAimEnabled = not silentAimEnabled
    AimButton.Text = silentAimEnabled and "[üì±]OFF Aim ModüéØ" or "[üì±]ON Aim ModüéØ"
    AimButton.BackgroundColor3 = silentAimEnabled and Color3.new(0, 1, 0) or Color3.new(0.2, 0.2, 0.2)

    -- Hi·ªán n√∫t Aim khi b·∫≠t ch·ª©c nƒÉng
    MobileAimButton.Visible = silentAimEnabled
    MobileAimButton.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
end

AimButton.MouseButton1Click:Connect(ToggleAim)

-- üü¢ GI·ªÆ N√öT AIM ƒê·ªÇ B·∫¨T AIM MOD
MobileAimButton.MouseButton1Down:Connect(function()
    if silentAimEnabled then
        isAimHeld = true
        MobileAimButton.BackgroundColor3 = Color3.fromRGB(0, 0, 139) -- Chuy·ªÉn sang m√†u xanh d∆∞∆°ng khi gi·ªØ
    end
end)

MobileAimButton.MouseButton1Up:Connect(function()
    isAimHeld = false
    MobileAimButton.BackgroundColor3 = Color3.fromRGB(100, 100, 100) -- Reset v·ªÅ m√†u x√°m
end)

-- üü¢ H√ÄM T√åM V√Ä AIM V√ÄO ZOMBIE
local function GetClosestZombieHeadInRange()
    local closestHead = nil
    local closestScreenDistance = math.huge
    local crosshairPosition = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y / 2)
    local maxRadius = 200

    local runtimeItemsFolder = workspace:FindFirstChild("RuntimeItems")

    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("Part") and obj.Name == "Head" and obj.Parent:IsA("Model") then
            local model = obj.Parent
            local isPlayer = game.Players:GetPlayerFromCharacter(model) ~= nil
            local isInRuntimeItems = runtimeItemsFolder and obj:IsDescendantOf(runtimeItemsFolder)

            if not isPlayer and not isInRuntimeItems then
                local screenPoint, onScreen = camera:WorldToViewportPoint(obj.Position)
                if onScreen then
                    local screenPosition = Vector2.new(screenPoint.X, screenPoint.Y)
                    local screenDistance = (screenPosition - crosshairPosition).magnitude
                    if screenDistance < closestScreenDistance and screenDistance <= maxRadius then
                        closestScreenDistance = screenDistance
                        closestHead = obj
                    end
                end
            end
        end
    end
    return closestHead
end

local function AimAtZombieHead()
    if not silentAimEnabled or not isAimHeld then return end

    local targetHead = GetClosestZombieHeadInRange()
    if targetHead then
        camera.CFrame = CFrame.new(camera.CFrame.Position, targetHead.Position)
    end
end

runService.RenderStepped:Connect(AimAtZombieHead)

-- üü¢ RESET TR·∫†NG TH√ÅI KHI CH·∫æT
player.CharacterAdded:Connect(function()
    silentAimEnabled = false
    isAimHeld = false
    AimButton.Text = "[üì±]ON Aim ModüéØ"
    AimButton.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
    MobileAimButton.Visible = false
    MobileAimButton.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
end)

