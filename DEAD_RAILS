--no clip
local HomeFrame = sections["Home"] -- L·∫•y ph·∫ßn ch·ª©a n·ªôi dung c·ªßa tab "Home"
local player = game.Players.LocalPlayer
local userInputService = game:GetService("UserInputService")
local runService = game:GetService("RunService")

local noclipEnabled = false
local menuVisible = false
local character = player.Character or player.CharacterAdded:Wait()

-- T·∫°o menu No Clip
local noclipMenu = Instance.new("Frame")
noclipMenu.Size = UDim2.new(0, 150, 0, 100)
noclipMenu.Position = UDim2.new(0.7, 0, 0.3, 0)
noclipMenu.BackgroundColor3 = Color3.new(0.0, 1.0, 1.0)
noclipMenu.Visible = false -- M·∫∑c ƒë·ªãnh ·∫©n
noclipMenu.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui"):WaitForChild("ScreenGui")

local toggleButton = Instance.new("TextButton", noclipMenu)
toggleButton.Size = UDim2.new(1, 0, 0.6, 0)
toggleButton.Position = UDim2.new(0, 0, 0.2, 0)
toggleButton.Text = "ON Noclip"
toggleButton.BackgroundColor3 = Color3.new(0.5, 0.5, 0.5)
toggleButton.TextColor3 = Color3.new(1, 1, 1)

-- T·∫°o n√∫t b·∫≠t menu No Clip trong tab Home
CreateButton(HomeFrame, "üß±NO CLIP MENU", 10, function()
    menuVisible = not menuVisible
    noclipMenu.Visible = menuVisible
end)

-- K√©o th·∫£ menu No Clip
local dragging
local dragInput
local dragStart
local startPos

local function updateInput(input)
    local delta = input.Position - dragStart
    noclipMenu.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

noclipMenu.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = noclipMenu.Position

        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

noclipMenu.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        dragInput = input
    end
end)

userInputService.InputChanged:Connect(function(input)
    if dragging and input == dragInput then
        updateInput(input)
    end
end)

-- H√†m b·∫≠t/t·∫Øt No Clip
local function setNoclip(state)
    if character then
        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = not state
            end
        end
    end
end

-- Khi b·∫•m n√∫t b·∫≠t/t·∫Øt
toggleButton.MouseButton1Click:Connect(function()
    noclipEnabled = not noclipEnabled
    toggleButton.Text = noclipEnabled and "OFF Noclip" or "ON Noclip"
    toggleButton.BackgroundColor3 = noclipEnabled and Color3.new(0, 1, 0) or Color3.new(0.5, 0.5, 0.5)

    if noclipEnabled then
        runService.Stepped:Connect(function()
            if noclipEnabled and character then
                setNoclip(true)
            end
        end)
    else
        setNoclip(false)
    end
end)

-- Khi nh√¢n v·∫≠t h·ªìi sinh, reset No Clip v·ªÅ ban ƒë·∫ßu
player.CharacterAdded:Connect(function(newCharacter)
    character = newCharacter
    noclipEnabled = false -- T·∫Øt No Clip ho√†n to√†n
    toggleButton.Text = "ON Noclip" -- ƒê·∫∑t l·∫°i n√∫t
    toggleButton.BackgroundColor3 = Color3.new(0.5, 0.5, 0.5) -- M√†u g·ªëc
    setNoclip(false) -- ƒê·∫£m b·∫£o nh√¢n v·∫≠t kh√¥ng c√≤n No Clip
end)

