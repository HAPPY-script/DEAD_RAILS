--ESP ITEM
local HomeFrame = sections["Visual"] -- L·∫•y ph·∫ßn ch·ª©a n·ªôi dung c·ªßa tab "Visual"
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local userInputService = game:GetService("UserInputService")

local espItemEnabled = false
local itemMenuVisible = false
local espItemObjects = {}
local childAddedConnection = nil

-- N√∫t m·ªü menu ESP Item
local espItemButton = Instance.new("TextButton", HomeFrame)
espItemButton.Size = UDim2.new(0, 300, 0, 40)
espItemButton.Position = UDim2.new(0, 10, 0, 60)
espItemButton.Text = "üíéMENU ESP ITEM"
espItemButton.BackgroundColor3 = Color3.new(0, 0.39, 0.31)
espItemButton.TextColor3 = Color3.new(150, 255, 150)
espItemButton.BorderSizePixel = 2

-- T·∫°o menu ESP Item
local playerGui = player:WaitForChild("PlayerGui")
local screenGui = Instance.new("ScreenGui", playerGui)
screenGui.Enabled = false

local espItemMenu = Instance.new("Frame", screenGui)
espItemMenu.Size = UDim2.new(0, 150, 0, 100)
espItemMenu.Position = UDim2.new(0.5, -75, 0.3, 0)
espItemMenu.BackgroundColor3 = Color3.new(0, 255, 0)
espItemMenu.BorderSizePixel = 2

